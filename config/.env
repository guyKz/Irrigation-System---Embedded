# TinkerCAD to ThingsBoard Bridge - Environment Variables
# Copy this file to .env and fill in your actual values

# ===== TinkerCAD Login Credentials =====
# Replace these with your actual TinkerCAD class and login codes
TINKERCAD_CLASS_CODE= 6YWGRSYQF
TINKERCAD_LOGIN_CODE= vladi

# ===== ThingsBoard Configuration =====
# Your ThingsBoard device access token (already configured, but you can change it)
TB_TOKEN=CY1zWMnlhydkKr0Hx9rG
TB_HOST=eu.thingsboard.cloud
USE_HTTPS=true

# ===== Bridge Behavior Settings =====
# Maximum rate to send telemetry to ThingsBoard (messages per second)
MAX_SEND_HZ=5.0

# How often to poll the serial monitor (seconds)
POLL_SEC=0.3

# Print each data payload to console before sending
PRINT_PREVIEW=true

# Run browser in headless mode (no visible window)
HEADLESS=false

# ===== Logging Configuration =====
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO